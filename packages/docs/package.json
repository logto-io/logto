{
  "name": "@logto/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "pnpm build:api && docusaurus start",
    "build": "pnpm build:api && docusaurus build",
    "build:api": "redoc-cli build https://logto.dev/api/swagger.json -o static/api/index.html --title 'Logto Core API Reference'",
    "swizzle": "docusaurus swizzle",
    "deploy": "pnpm build:api && docusaurus deploy",
    "clear": "rm static/api/index.html && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.13.0",
    "@docusaurus/core": "2.0.0-beta.21",
    "@docusaurus/module-type-aliases": "2.0.0-beta.21",
    "@docusaurus/preset-classic": "2.0.0-beta.21",
    "@docusaurus/theme-classic": "2.0.0-beta.21",
    "@docusaurus/types": "2.0.0-beta.21",
    "@mdx-js/react": "^1.6.22",
    "@docusaurus/theme-common": "2.0.0-beta.21",
    "@silverhand/eslint-config": "^0.14.0",
    "@silverhand/eslint-config-react": "^0.14.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.14",
    "clsx": "^1.1.1",
    "core-js": "^3.1.4",
    "docusaurus-plugin-sass": "^0.2.2",
    "eslint": "^8.10.0",
    "postcss": "^8.4.6",
    "prettier": "^2.3.2",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^16.8.0",
    "redoc-cli": "^0.13.16",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.8.2",
    "typescript": "^4.6.3",
    "webpack": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
