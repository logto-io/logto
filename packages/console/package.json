    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-animate-height": "^3.0.4",
    "react-dnd": "^16.0.0",
    "react-dnd-html5-backend": "^16.0.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-hot-toast": "^2.2.0",
    "react-i18next": "^11.15.4",
